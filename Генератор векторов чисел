#include <iostream>
#define N_MAX 64
int m[N_MAX];

void f(int i, int n) {
if (i < n) {
for (int v = 4; v < 8; v++) {
m[i] = v;
if (!(i >= 2 && m[i] >= m[i - 1] && m[i - 1] >= m[i - 2])) {
f(i + 1, n);
}
}
}
else {
for (int j = 0; j < n; j++) {
std::cout « m[j] « " ";
}
std::cout « "\n";
}
}

int main() {
f(0, 4);
return 0;
}
